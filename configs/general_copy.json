{
    "dconf": {
        "source": "/general/data/dconf/",
        "destination": "/etc/dconf/"
    },
    "AccountsService": {
        "source": "/general/data/AccountsService/",
        "destination": "/var/lib/AccountsService/"
    },
    "DesktopEntries-Admin": {
        "source": "/general/data/DesktopEntries/",
        "destination": "/home/admin/.local/share/applications/",
        "permissions": {
            "uid": 1000,
            "gid": 1000
        }
    },
    "DesktopEntries-User": {
        "source": "/general/data/DesktopEntries/",
        "destination": "/home/user/.local/share/applications/",
        "permissions": {
            "uid": 1001,
            "gid": 1001
        }
    },
    "Icons": {
        "source": "/general/data/images/icons/",
        "destination": "/var/lib/AccountsService/icons/"
    },
    "Logos": {
        "source": "/general/data/images/logos/",
        "destination": "/usr/share/logos/"
    },
    "X11-VT-Switch": {
        "source": "/general/data/01-vt-switch.conf",
        "destination": "/etc/X11/xorg.conf.d/01-vt-switch.conf"
    },
    "Gdm-conf": {
        "source": "/general/data/custom.conf",
        "destination": "/etc/gdm/custom.conf"
    },
    "Grub-conf": {
        "source": "/general/data/grub",
        "destination": "/etc/default/grub"
    },
    "Environment-Variable-User": {
        "source": "/general/data/environmentVariableUser.conf",
        "destination": "/home/user/.config/environment.d/variable.conf",
        "permissions": {
            "uid": 1001,
            "gid": 1001
        }
    },
    "Environment-Variable-Admin": {
        "source": "/general/data/environmentVariableAdmin.conf",
        "destination": "/home/admin/.config/environment.d/variable.conf",
        "permissions": {
            "uid": 1000,
            "gid": 1000
        }
    }
}