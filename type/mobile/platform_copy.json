{
    "Autostart": {
        "source": "/type/mobile/data/myWorkspaceAutostart.desktop",
        "destination": "/etc/xdg/autostart/myWorkspaceAutostart.desktop"
    },
    "Dconf": {
        "source": "/type/mobile/data/dconf",
        "destination": "/etc/dconf/"
    },
    "MyWorkspaceDesktop-User": {
        "source": "/type/mobile/data/myWorkspace.desktop",
        "destination": "/home/user/.local/share/applications/myWorkspace.desktop"
    },
    "MyWorkspaceDesktop-Admin": {
        "source": "/type/mobile/data/myWorkspace.desktop",
        "destination": "/etc/dconf/"
    },
    "Firefox-Share": {
        "source": "/type/mobile/data/firefox/share",
        "destination": "/usr/share/firefox/"
    },
    "Firefox-Policies": {
        "source": "/type/mobile/data/firefox/policies.json",
        "destination": "/etc/firefox/policies/"
    },
    "Wifi": {
        "source": "/type/mobile/data/wifi_backend.conf",
        "destination": "/etc/NetworkManager/conf.d/wifi_backend.conf"
    },
    "Systemd-conf": {
        "type": "file",
        "source": "/type/mobile/data/systemd_logind.conf",
        "destination": "/etc/systemd/logind.conf"
    }
}